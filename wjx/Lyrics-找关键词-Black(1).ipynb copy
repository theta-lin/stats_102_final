    "BandName=[]                            # 乐队名字\n",
    "for idx in range(df.shape[0]):\n",
    "    BandName.append(df.iloc[idx][2])   # 乐队名字\n",
    "    String=df.iloc[idx][7]             # 歌词  \n",
    "    String= String.lower()\n",
    "    \n",
    "   \n",
    "    for i in range(len(KeyWord)):\n",
    "        for j in range(len(KeyWord[i])):\n",
    "            if KeyWord[i][j] in String:   \n",
    "                KWCnt[idx][i]=KWCnt[idx][i]+1\n",
    "       \n",
    "        \n",
    "# 转换为百分比            \n",
    "KWRto=np.copy(KWCnt)\n",
    "KWRto=KWRto.astype(np.float64)\n",
    "display(KWRto)\n",
    "a=np.sum(KWRto,axis=1)\n",
    "display(a)\n",
    "NoKeyWordCnt=0            # 没有关键词的歌曲数目\n",
    "NoKeyWordArr=[]           # 没有关键词的歌曲序号\n",
    "for i in range(KWRto.shape[0]):\n",
    "    if a[i]<4:\n",
    "        NoKeyWordCnt=NoKeyWordCnt+1   # 少于4个就认为是无关键词\n",
    "        NoKeyWordArr= NoKeyWordArr+[i]\n",
    "    else:\n",
    "        for j in range(KWRto.shape[1]):\n",
    "            KWRto[i][j]/=a[i]\n",
    "            \n",
    "# 打印：没有关键词的歌曲数目，占歌曲总数的百分比\n",
    "print('\\nNoKeyWordCnt: ',NoKeyWordCnt,'/',KWRto.shape[0],'=',NoKeyWordCnt/KWRto.shape[0],'\\n')\n",
    "\n",
    "for i in range(len(KeyWord)):\n",
    "    print('Colum',i,'=',KeyWord[i])\n",
    "    \n",
    "                   \n",
    "print('\\nKWCnt=',len(KWCnt))\n",
    "print(KWCnt)\n",
    "\n",
    "print('\\nKWRto=',len(KWRto))\n",
    "print(KWRto)\n",
    "\n",
    "# 保存到文件（各类关键词数量）\n",
    "df1=pd.DataFrame(KWCnt)\n",
    "df1['BandName']=BandName              # 增加一列：乐队名字\n",
    "df1.drop(NoKeyWordArr,inplace=True)   # 去掉没有关键词的歌曲\n",
    "df1.to_csv('KeyWord1.csv', header=None)"
   ]
